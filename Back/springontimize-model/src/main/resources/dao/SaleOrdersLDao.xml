<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup
        xmlns="http://www.ontimize.com/schema/jdbc"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd"
        catalog="" schema="${mainschema}" table="saleordersl"
        datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
    <DeleteKeys>
        <Column>id</Column>
    </DeleteKeys>
    <UpdateKeys>
        <Column>id</Column>
    </UpdateKeys>
    <GeneratedKey>id</GeneratedKey>
    <Queries>
        <Query id="default">
            <AmbiguousColumns>
                <AmbiguousColumn name="product_id" prefix="PO" databaseName="id" />
                <AmbiguousColumn name="product_price" prefix="PO" databaseName="price" />
                <AmbiguousColumn name="sale_id" prefix="SA" databaseName="id" />
                <AmbiguousColumn name="sale_price" prefix="SA" databaseName="price" />
                <AmbiguousColumn name="sale_user" prefix="SA" databaseName="user_" />
                <AmbiguousColumn name="tuser_user" prefix="TU" databaseName="user_" />
            </AmbiguousColumns>
            <Sentence>
                <![CDATA[
               SELECT
                  #COLUMNS#
               FROM
                  ${mainschema}.SALES AS SA
                  INNER JOIN ${mainschema}.PRODUCTS AS PO ON PO.ID = SA.PRODUCT_ID
                  INNER JOIN ${mainschema}.TUSER AS TU ON TU.USER_ = SA.USER_
               #WHERE#
               #ORDER#
            ]]>
            </Sentence>
        </Query>
    </Queries>
</JdbcEntitySetup>